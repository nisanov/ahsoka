{
    "name": "nisanov/ahsoka",
    "description": "Ahsoka.",
    "keywords": ["ahsoka"],
    "homepage": "https://github.com/nisanov/ahsoka",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/nisanov/ahsoka/issues",
        "source": "https://github.com/nisanov/ahsoka"
    },
    "authors": [
        {
            "name": "Yasha Nisanov",
            "email": "yasha.nisanov@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "laminas/laminas-text": "^2.10",
        "laravel/pint": "^1.13",
        "laravel-zero/framework": "^10.2",
        "laravel-zero/phar-updater": "^1.3",
        "mockery/mockery": "^1.6",
        "nunomaduro/termwind": "^1.15.1",
        "pestphp/pest": "^2.22"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["builds/ahsoka"]
}
